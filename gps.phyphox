  <input>
    <location>
      <output component="lat">lat</output>
      <output component="lon">lon</output>
      <output component="z">z</output>
      <output component="v">v</output>
      <output component="dir">dir</output>
      <output component="accuracy">accuracy</output>
      <output component="zAccuracy">zAccuracy</output>
      <output component="t">t</output>
      <output component="satellites">satellites</output>
      <output component="status">status</output>
    </location>
  </input>
  <views>
    <view label="Graph">
      <value label="Status">
        <input>status</input>
        <map max="-1">GPS disabled</map>
        <map min="0">GPS active</map>
      </value>
      <separator height="1"/>
      <graph label="Latitude" labelX="t" unitX="s" labelY="φ" unitY="°" partialUpdate="true">
        <input axis="x">t</input>
        <input axis="y">lat</input>
      </graph>
      <graph label="Longitude" labelX="t" unitX="s" labelY="λ" unitY="°" partialUpdate="true">
        <input axis="x">t</input>
        <input axis="y">lon</input>
      </graph>
      <graph label="Height" labelX="t" unitX="s" labelY="z" unitY="m" partialUpdate="true">
        <input axis="x">t</input>
        <input axis="y">z</input>
      </graph>
      <graph label="Speed" labelX="t" unitX="s" labelY="v" unitY="m/s" partialUpdate="true">
        <input axis="x">t</input>
        <input axis="y">v</input>
      </graph>
      <graph label="Direction" labelX="t" unitX="s" labelY="Direction" unitY="°" partialUpdate="true" style="dots">
        <input axis="x">t</input>
        <input axis="y">dir</input>
      </graph>
      <graph label="Distance" labelX="t" unitX="s" labelY="d" unitY="km" partialUpdate="true">
        <input axis="x">t</input>
        <input axis="y">dist</input>
      </graph>
    </view>
    <view label="Simple">
      <value label="Status">
        <input>status</input>
        <map max="-1">GPS disabled</map>
        <map min="0">GPS active</map>
      </value>
      <separator height="1"/>
      <value label="Latitude" size="1" precision="6" unit="°">
        <input>lat</input>
      </value>
      <value label="Longitude" size="1" precision="6" unit="°">
        <input>lon</input>
      </value>
      <value label="Height" size="1" precision="1" unit="m">
        <input>z</input>
      </value>
      <separator height="1"/>
      <value label="Speed" size="1" precision="1" unit="m/s">
        <input>v</input>
      </value>
      <value label="Speed" size="1" precision="1" factor="3.6" unit="km/h">
        <input>v</input>
      </value>
      <separator height="1"/>
      <value label="Direction" size="1" precision="1" unit="°">
        <input>dir</input>
      </value>
      <value label="Compass" size="1">
        <input>dir</input>
        <map max="-157.5">S</map>
        <map max="-112.5">SW</map>
        <map max="-67.5">W</map>
        <map max="-22.5">NW</map>
        <map max="22.5">N</map>
        <map max="67.5">NE</map>
        <map max="112.5">E</map>
        <map max="157.5">SE</map>
        <map max="202.5">S</map>
        <map max="247.5">SW</map>
        <map max="292.5">W</map>
        <map max="337.5">NW</map>
        <map>N</map>
      </value>
      <separator height="1"/>
      <value label="Distance" size="1" precision="3" unit="km">
        <input>dist</input>
      </value>
      <separator height="1"/>
      <value label="Horizontal Accuracy" size="1" precision="1" unit="m">
        <input>accuracy</input>
      </value>
      <value label="Vertical Accuracy" size="1" precision="1" unit="m">
        <input>zAccuracy</input>
      </value>
      <value label="Satellites" size="1" precision="0">
        <input>satellites</input>
      </value>
      <separator height="1"/>
      <info label="Note, that some devices do not have a GPS sensor, but provide location data through mobile or WIFI connections. These data sources produce data with bad accuracy."/>
      <separator height="1"/>
      <info label="Also make sure that your system's location setting is not set to &quot;Power saving&quot; because this usually turns off GPS while navigation apps might still show a vague location determined through the network."/>
    </view>
  </views>
  <analysis sleep="0.1">
    <count>
      <input clear="false">t</input>
      <output clear="false">countT</output>
    </count>
    <count>
      <input clear="false">d</input>
      <output>countUpdated</output>
    </count>
    <add>
      <input clear="false">countUpdated</input>
      <input type="value">1</input>
      <output>countUpdated+1</output>
    </add>
    <subrange>
      <input clear="false" as="from">countUpdated</input>
      <input clear="false" as="in">lat</input>
      <output>subLat</output>
    </subrange>
    <subrange>
      <input clear="false" as="from">countUpdated</input>
      <input clear="false" as="in">lon</input>
      <output>subLon</output>
    </subrange>
    <differentiate>
      <input clear="false">subLat</input>
      <output>dLat</output>
    </differentiate>
    <differentiate>
      <input clear="false">subLon</input>
      <output>dLon</output>
    </differentiate>
    <cos deg="true">
      <input clear="false">subLat</input>
      <output>cosLat</output>
    </cos>
    <multiply>
      <input clear="false">dLon</input>
      <input clear="false">dLon</input>
      <input clear="false">cosLat</input>
      <input clear="false">cosLat</input>
      <output>dLon2</output>
    </multiply>
    <multiply>
      <input clear="false">dLat</input>
      <input clear="false">dLat</input>
      <output>dLat2</output>
    </multiply>
    <add>
      <input clear="false">dLon2</input>
      <input clear="false">dLat2</input>
      <output>dphi2</output>
    </add>
    <power>
      <input as="base" clear="false">dphi2</input>
      <input as="exponent" type="value">0.5</input>
      <output>dphi</output>
    </power>
    <multiply>
      <input clear="false">dphi</input>
      <input type="value">6371</input>
      <input type="value">0.017453293</input>
      <output>dnew</output>
    </multiply>
    <if greater="true">
      <input clear="false">countT</input>
      <input clear="false">countUpdated+1</input>
      <input clear="false">dnew</input>
      <output clear="false">d</output>
    </if>
    <integrate>
      <input clear="false">d</input>
      <output>dist</output>
    </integrate>
  </analysis>
  <export>
    <set name="Raw Data">
      <data name="Time (s)">t</data>
      <data name="Latitude (°)">lat</data>
      <data name="Longitude (°)">lon</data>
      <data name="Height (m)">z</data>
      <data name="Speed (m/s)">v</data>
      <data name="Direction (°)">dir</data>
      <data name="Distance (km)">dist</data>
      <data name="Horizontal Accuracy (m)">accuracy</data>
      <data name="Vertical Accuracy (m)">zAccuracy</data>
      <data name="Satellites">satellites</data>
    </set>
  </export>
</phyphox>
